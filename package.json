{
    "name": "rstart",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbopack",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "prepare": "husky install",
        "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
        "db:push": "drizzle-kit push --config=drizzle.config.ts",
        "db:auth:generate": "pnpm dlx @better-auth/cli generate --output src/lib/db/auth-schema.ts -y"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.74.3",
        "@trpc/client": "^11.1.0",
        "@trpc/react-query": "^11.1.0",
        "@trpc/server": "^11.1.0",
        "better-auth": "^1.2.7",
        "class-variance-authority": "^0.7.1",
        "client-only": "^0.0.1",
        "clsx": "^2.1.1",
        "drizzle-orm": "^0.42.0",
        "lucide-react": "^0.488.0",
        "next": "15.3.0",
        "next-themes": "^0.4.6",
        "pg": "^8.14.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "server-only": "^0.0.1",
        "superjson": "^2.2.2",
        "tailwind-merge": "^3.2.0",
        "tw-animate-css": "^1.2.5",
        "zod": "^3.24.2"
    },
    "devDependencies": {
        "@better-auth/cli": "^1.2.7",
        "@eslint/eslintrc": "^3",
        "@tailwindcss/postcss": "^4.1.4",
        "@types/node": "^20",
        "@types/pg": "^8.11.13",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "@typescript-eslint/eslint-plugin": "^8.30.1",
        "drizzle-kit": "^0.31.0",
        "eslint": "^9",
        "eslint-config-next": "15.3.0",
        "eslint-config-prettier": "^10.1.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.6",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-react": "^7.37.5",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.1",
        "prettier": "^3.5.3",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "tailwindcss": "^4.1.4",
        "typescript": "^5"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "@prisma/client",
            "@prisma/engines",
            "better-sqlite3",
            "esbuild",
            "prisma",
            "sharp"
        ]
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,css,md,json,html,mdx}": "prettier --config ./prettier.config.mjs --write"
    }
}
